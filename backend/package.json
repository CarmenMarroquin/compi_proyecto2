{
  "name": "backend",
  "version": "0.0.1",
  "main": "build/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --forceExit --runInBand --detectOpenHandles --config=jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch --watchAll --forceExit --runInBand --detectOpenHandles --config=jest.config.js",
    "start": "node build/index",
    "build": "tsc",
    "dev": "ts-node-dev src/index",
    "jison": "jison -t tsconfig.json -n CompInterpreter -n CompInterpreter -o src/analizador/gramatica.ts src/analizador/gramatica.jison",
    "ts-jison": "ts-jison -t typescript -n CompInterpreter -n CompInterpreter -o src/analizador/gramatica.ts src/analizador/gramatica.jison",
    "ts-jison:debug": "jison -t typescript -n CIParser -n CIParser -o src/analizador/gramatica.ts src/analizador/gramatica.jison --debug"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@jest/globals": "^29.7.0",
    "@ts-jison/parser-generator": "^0.4.1-alpha.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/joi": "^17.2.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/supertest": "^6.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.21.0",
    "jest": "^29.7.0",
    "jison": "^0.4.18",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-jison": "^0.0.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
